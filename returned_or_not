SELECT start_station,

ROUND(100 * CAST(COUNT(CASE WHEN (start_station = end_station) THEN 1 ELSE NULL END) AS float) / CAST(COUNT(*) AS float), 2)
    as perc_return,

ROUND(100 * CAST(COUNT(CASE WHEN (start_station <> end_station) THEN 1 ELSE NULL END) AS float) / CAST(COUNT(*) AS float), 2) 
    as perc_else

FROM trips
GROUP BY start_station
order by perc_return desc


